{
	"apiVersion": 2,
	"name": "core/pullquote",
	"category": "text",
	"attributes": {
		"value": {
			"type": "string",
			"source": "html",
			"selector": "blockquote",
			"multiline": "p"
		},
		"citation": {
			"type": "string",
			"source": "html",
			"selector": "cite",
			"default": ""
		},
		"mainColor": {
			"type": "string"
		},
		"customMainColor": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"customTextColor": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"align": [
			"left",
			"right",
			"wide",
			"full"
		],
		"color": {
		  "gradients": false,
		  "background": false,
			"__experimentalSkipSerialization": true
		},
		"__experimentalBorder": {
		  "color": true,
		  "radius": false,
		  "__experimentalSkipSerialization": true
		}
	},
  	"__experimentalSelector": ".wp-block-pullquote blockquote",
	"editorStyle": "wp-block-pullquote-editor",
	"style": "wp-block-pullquote"
}
